{% extends "@Contao/content_element/youtube.html.twig" %}

{% block iframe %}
    {% set iframe_attrs = attrs({width, height, 'data-src': source.url, 'data-name': 'youtube-video', allowfullscreen: true})
        .mergeWith(iframe_attrs|default)
    %}
    <iframe{{ iframe_attrs }}></iframe>
{% endblock %}
